{
  "name": "calculator-pwa",
  "version": 2,
  "public": true,
  "alias": "calculator-pwa.now.sh",
  "regions": ["all"],
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    { "src": "^/profile$", "dest": "/index.html" },
    { "src": "^/profile/john$", "dest": "/index.html" },
    { "src": "/$", "dest": "/index.html" }
  ],
  "github": {
    "silent": true
  }
}