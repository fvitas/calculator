{
  "private": false,
  "name": "calculator-pwa",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf ./dist ./.cache",
    "dev": "npm run clean && npx parcel ./src/index.html --no-hmr && npx workbox injectManifest",
    "build": "npm run clean && npx parcel build ./src/index.html -d ./dist --public-url ./ &&  npx workbox injectManifest",
    "now-build": "NODE_ENV=production npm run build",
    "deploy": "now"
  },
  "browserslist": [
    "last 2 android versions"
  ],
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "^9.4.6",
    "now": "^13.1.2",
    "postcss-modules": "1.4.1",
    "stylus": "^0.54.5",
    "workbox-cli": "^4.0.0-beta.2"
  },
  "dependencies": {
    "parcel-bundler": "1.11.0",
    "preact": "8.4.2",
    "preact-router": "2.6.1"
  },
  "author": "Filip Vitas",
  "license": "MIT"
}
